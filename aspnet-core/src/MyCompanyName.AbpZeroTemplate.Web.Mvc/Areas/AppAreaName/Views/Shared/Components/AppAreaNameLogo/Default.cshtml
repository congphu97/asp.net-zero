@model MyCompanyName.AbpZeroTemplate.Web.Areas.AppAreaName.Models.Layout.LogoViewModel
@{
    Layout = null;
    var theme = await GetTheme();
}

<img alt="Logo" src="@Model.GetLogoUrl(ApplicationPath, theme.IsTopMenuUsed || theme.IsTabMenuUsed ? theme.BaseSettings.Header.HeaderSkin: theme.BaseSettings.Menu.AsideSkin)" height="38" class="kt-header__brand-logo-default" />
